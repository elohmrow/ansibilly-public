<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>author-bootstraps</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <versionHandler>info.magnolia.training.setup.AuthorBootstrapsVersionHandler</versionHandler>
  <version>${project.version}</version>

  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.5.6/*</version>
    </dependency>

    <!-- without this here, the license file doesn't get bootstrapped due to module loading order. -->
    <dependency>
      <name>enterprise</name>
      <version>5.5.6/*</version>
    </dependency>

  </dependencies>
</module>